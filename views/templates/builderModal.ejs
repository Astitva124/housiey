<div class="modal fade" id="add-builder" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Add Builder</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <form>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="mb-3">
                                <label for="verticalnav-firstname-input">Builder name*</label>
                                <input type="text" class="form-control" id="verticalnav-firstname-input" placeholder="Enter Your Project Name">
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="mb-3">
                                <label class="form-label">City*</label>
                                <select class="form-select">
                                    <option>Select</option>
                                    <option>Large select</option>
                                    <option>Small select</option>
                                </select> 
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="mb-3">
                                <label for="verticalnav-address-input">Builder Description</label>
                                <textarea id="verticalnav-address-input" class="form-control" rows="2" placeholder="Enter Your Address"></textarea>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-6">
                            <div class="mb-3">
                                <label for="verticalnav-phoneno-input">Established Year</label>
                                <input class="form-control" type="month" value="2019-08" id="example-month-input">
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="mb-3">
                                <label for="verticalnav-phoneno-input">Total Experience</label>
                                <input type="number" class="form-control" id="verticalnav-phoneno-input" placeholder="only numbers">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="mb-3">
                                <label for="verticalnav-phoneno-input">Total Complete Projects</label>
                                <input type="number" class="form-control" id="verticalnav-phoneno-input" placeholder="only numbers">
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="mb-3">
                                <label for="verticalnav-phoneno-input">Happy Families</label>
                                <input type="number" class="form-control" id="verticalnav-phoneno-input" placeholder="only numbers">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="mb-3">
                                <label for="verticalnav-address-input">Upload Logo</label>
                                <input class="form-control" type="file" id="formFile">
                            </div>
                        </div>
                    </div>                 
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-light" data-bs-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save</button>
                </div>
            </form>
        </div>
    </div>
</div>